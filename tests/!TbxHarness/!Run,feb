set TbxHarness$Dir <Obey$Dir>

rmensure Toolbox 0.00 rmload System:Modules.Toolbox.Toolbox
rmensure Menu 0.00 rmload System:Modules.Toolbox.Menu
rmensure Treeview 0.19 rmload System:Modules.Toolbox.TreeView
rmensure Tabs 0.09 rmload System:Modules.Toolbox.Tabs

wimpslot -min 1024k -max 1024k
/<TbxHarness$Dir>.!RunImage %*0